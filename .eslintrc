{
  "parser": "babel-eslint",
  "extends": [
    "plugin:jest/recommended",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/exports-last": 2,
    "react/destructuring-assignment": [true, "always", { "ignoreClassFields": true }]
  },
  "env": {
    "browser": true,
    "node": true
  }
}